{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "outDir": "./lib/",
    "moduleResolution": "Node",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src"]
}